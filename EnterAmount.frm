Private Sub Command13_Click()
If AtmNumber.AccountBalance >= 200 And Val(Text1.Text) <= 20000 And (Val(Text1.Text) Mod 100 = 0) And Val(Text1.Text) < AtmNumber.AccountBalance Then
Conn.Open "Provider=Microsoft.Jet.OLEDB.4.0;Data Source=E:\vbProject\Database\ATM.mdb;Persist Security Info=False"

EnterAmountBalance = Val(Text1.Text)
EnterAmountType = "Transfer"
EnterAmountDate = Format(Now, "dd/mm/yyyy")
AtmNumber.AccountBalance = AtmNumber.AccountBalance - Val(Text1.Text)
TransferCheck.TransferBalance = TransferCheck.TransferBalance + Val(Text1.Text)
s1 = "update Account set Balance=" & AtmNumber.AccountBalance & " where Acc_No= " & AtmNumber.AccountNo & ""
s2 = "update Account set Balance=" & TransferCheck.TransferBalance & " where Acc_no= " & TransferCheck.TransferAccountNo & " "
s3 = "insert into Trans(T_Type,Acc_No,DOT,Narration,Amount) values('" & EnterAmountType & "'," & AtmNumber.AccountNo & ",'" & EnterAmountDate & "','" & EnterAmountNarration1 & "'," & EnterAmountBalance & ")"
s4 = "insert into Trans(T_Type,Acc_No,DOT,Narration,Amount) values('" & EnterAmountType & "'," & TransferCheck.TransferAccountNo & ",'" & EnterAmountDate & "','" & EnterAmountNarration2 & "'," & EnterAmountBalance & ")"
Record.Open s1, Conn, adOpenDynamic, adLockOptimistic
Record.Open s2, Conn, adOpenDynamic, adLockOptimistic
Record.Open s3, Conn, adOpenDynamic, adLockOptimistic
Record.Open s4, Conn, adOpenDynamic, adLockOptimistic
PleaseWait.Timer1.Enabled = True
PleaseWait.Timer2.Enabled = True
PleaseWait.Timer3.Enabled = True
PleaseWait.Show
EnterAmount.Hide
Conn.Close
Else
Invalid.Show
Invalid.Label1.Caption = "Insufficient Amount"
Invalid.Timer1.Enabled = True
EnterAmount.Hide
End If
End Sub

