Private Sub Command13_Click()
Conn.Open "Provider=Microsoft.Jet.OLEDB.4.0;Data Source=E:\vbProject\Database\ATM.mdb;Persist Security Info=False"

If Services.flag = 1 Then
s = "Select Bill_Amount,Consumer_Name,Status from BillPay where Consumer_No= " & Val(Text1.Text) & " "
Record.Open s, Conn, adOpenDynamic, adLockOptimistic
If Record.EOF Then
Invalid.Timer1.Enabled = True
Invalid.Label1.Caption = "Invalid Bill no"
Invalid.Show
ServicesPay.Hide
ElseIf Record.Fields(2) = "completed" Then
Invalid.Timer1.Enabled = True
Invalid.Label1.Caption = "Bill Already Paid"
Invalid.Show
ServicesPay.Hide

Else
flag = 1
BillAmount = Record.Fields(0)
ServicesAmount.Label1.Caption = BillAmount
ServicesAmount.Label5.Caption = Record.Fields(1)
ServicesAmount.Show
ServicesPay.Hide
End If
ElseIf Services.flag = 2 Then
s = "Select Mobile_Bill,Customer_Name,Status from MobilePay where Mobile_No= '" & Text1.Text & "'"
Record.Open s, Conn, adOpenDynamic, adLockOptimistic
If Record.EOF Then
Invalid.Timer1.Enabled = True
Invalid.Label1.Caption = "Invalid Mobile no"
Invalid.Show
ServicesPay.Hide
ElseIf Record.Fields(2) = "completed" Then
Invalid.Timer1.Enabled = True
Invalid.Label1.Caption = " Mobile Bill already paid"
Invalid.Show
ServicesPay.Hide

Else
flag = 2
MobileBill = Record.Fields(0)
ServicesAmount.Label1.Caption = MobileBill
ServicesAmount.Label5.Caption = Record.Fields(1)
ServicesAmount.Show
ServicesPay.Hide
End If
End If
Conn.Close
End Sub
